# Stories API 설정
# 사용할 소스 선택: 'notion', 'rss', 'github'
NEXT_PUBLIC_STORIES_SOURCE=notion

# Notion API 설정 (Notion 사용 시)
NOTION_API_KEY=your_notion_api_key_here
NOTION_DATABASE_ID=your_notion_database_id_here

# RSS Feed 설정 (RSS 사용 시)
RSS_FEED_URL=https://blog.naver.com/rss/your_blog_id

# GitHub 설정 (GitHub 사용 시)
GITHUB_REPO=username/repo-name
GITHUB_TOKEN=your_github_token_here

# 이메일 발송 설정 (sogo mail)
SMTP_HOST=smtp.sogo.co.kr
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=support@teamcodebridge.dev
SMTP_PASS=your_email_password_or_app_password

# Google OAuth 설정
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=GOCSPX-your_google_client_secret
NEXTAUTH_SECRET=your_nextauth_secret_key_here
NEXTAUTH_URL=https://e2g.teamcodebridge.dev

# Google Drive API 설정 (클라이언트 사이드)
NEXT_PUBLIC_GOOGLE_API_KEY=your_google_api_key_here
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
NEXT_PUBLIC_DRIVE_FOLDER_ID=1tZGBH3CXom6Hj7p5EyjKqDrzi-O4v99I


